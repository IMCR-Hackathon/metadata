 formats."}],"http://ontosoft.org/software#hasSoftwareCategory":[{"@type":"EnumerationEntity","id":"http://imcr.ontosoft.org/repository/software/enumerations#SoftwareCategory-gtib5Wlg3FGZ","name":"SoftwareCategory-gtib5Wlg3FGZ","type":"http://ontosoft.org/software#SoftwareCategory","label":"quality control","value":"quality control"},{"@type":"EnumerationEntity","id":"http://imcr.ontosoft.org/repository/software/enumerations#SoftwareCategory-ETibAfFmiTaD","name":"SoftwareCategory-ETibAfFmiTaD","type":"http://ontosoft.org/software#SoftwareCategory","label":"integration","value":"integration"},{"@type":"EnumerationEntity","id":"http://imcr.ontosoft.org/repository/software/enumerations#SoftwareCategory-BhrsPrEIoumc","name":"SoftwareCategory-BhrsPrEIoumc","type":"http://ontosoft.org/software#SoftwareCategory","label":"validation","value":"validation"},{"@type":"EnumerationEntity","id":"http://imcr.ontosoft.org/repository/software/enumerations#SoftwareCategory-bR7VLgRGWmaC","name":"SoftwareCategory-bR7VLgRGWmaC","type":"http://ontosoft.org/software#SoftwareCategory","label":"assurance","value":"assurance"},{"@type":"EnumerationEntity","id":"http://imcr.ontosoft.org/repository/software/enumerations#SoftwareCategory-8wMj1c2V80io","name":"SoftwareCategory-8wMj1c2V80io","type":"http://ontosoft.org/software#SoftwareCategory","label":"cleaning","value":"cleaning"}],"http://ontosoft.org/software#hasSoftwareCommunity":[{"@type":"TextEntity","id":"http://imcr.ontosoft.org/repository/software/Software-aHnsa3WHfmf0#CommunityInformation-fE7WzRxoSIZL","name":"CommunityInformation-fE7WzRxoSIZL","type":"http://ontosoft.org/software#CommunityInformation","value":"https://github.com/cboettig/taxadb"}],"http://ontosoft.org/software#hasSoftwareSupport":[],"http://ontosoft.org/software#hasSoftwareVersion":[],"http://ontosoft.org/software#hasTestData":[],"http://ontosoft.org/software#hasTestInstructions":[],"http://ontosoft.org/software#hasUniqueId":[],"http://ontosoft.org/software#hasUsageI